              },
              "percentileRank": {
                "type": "double"
              }
            }
          }
        }
      },
      "metricExpression": {
        "type": "object",
        "properties": {
          "language": {
            "type": "keyword"
          },
          "code": {
            "type": "text",
            "analyzer": "om_analyzer"
          }
        }
      },
      "metricType": {
        "type": "keyword"
      },
      "unitOfMeasurement": {
        "type": "keyword"
      },
      "customUnitOfMeasurement": {
        "type": "keyword"
      },
      "granularity": {
        "type": "keyword"
      },
      "relatedMetrics": {
        "type": "keyword"
      }
    }
  }
}